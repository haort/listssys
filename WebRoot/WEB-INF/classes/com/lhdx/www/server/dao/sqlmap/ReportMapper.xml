<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lhdx.www.server.dao.ReportDao">
	<select id="selectReports" resultMap="reportMap"  >
	   select name,acc,addr,area,startdt,enddt,own from report
	</select>    
	 <resultMap type="Report" id="reportMap">
	     <id property="id" column="id" />  
	     <result property="name" column="name"/>  
	     <result property="acc" column="acc"/>
	     <result property="addr" column="addr"/>  
	     <result property="area" column="area"/>  
	     <result property="startdt" column="startdt"/>  
	     <result property="enddt" column="enddt"/>  
	     <result property="own" column="own"/>  
	</resultMap>
	
</mapper>